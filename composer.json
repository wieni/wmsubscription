{
    "name": "wieni/wmsubscription",
    "type": "drupal-module",
    "description": "Provides a common interface for managing email marketing platform subscriptions in Drupal 8",
    "license": "MIT",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "drupal/core": "^9.0 || ^10.0" || ^11.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.0",
        "wieni/wmcodestyle": "^1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\wmsubscription\\": "src/"
        }
    },
    "scripts": {
        "coding-standards": [
            "@composer normalize",
            "php-cs-fixer fix --config=.php_cs.php",
            "phpstan analyse"
        ]
    }
}
